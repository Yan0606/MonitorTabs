{
    "manifest_version": 3,
    "name": "Alternador de Guias",
    "version": "1.0",
    "description": "Abre e alterna entre guias automaticamente.",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon.png"
      }
    }
  }
  